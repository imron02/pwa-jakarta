{
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true,
    "es6": true
  },
  "plugins": [
    "react"
  ],
  "extends": "airbnb",
  "rules": {
    "indent": ["error", 2],
    "comma-dangle": ["error", "never"],
    "jsx-a11y/anchor-is-valid": false,
    "react/jsx-one-expression-per-line": 0
  }
}